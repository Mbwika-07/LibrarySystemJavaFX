<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="application.controller.RemoveUsersController">
    <VBox alignment="CENTER" spacing="10.0">
        <TextField fx:id="usernameField" promptText="Username"/>
        <TextField fx:id="idField" promptText="ID"/>
        <Button text="Remove User" onAction="#handleRemoveUser"/>
        <Button text="Back to Main" onAction="#handleBackToMain"/>
    </VBox>
</AnchorPane>
